
public class VarEx3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		final int score = 100;
//		score = 200;
		boolean power = false;
		
		byte b = 127; // byte 의 범위 -128 ~ 127
		
		int oct = 010; // 8진수, 10진수로는 8
		int hex = 0x10; // 16진수, 10진수로는 16
		System.out.println(hex);
	}

}
